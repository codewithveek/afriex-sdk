{
    "name": "@afriex/core",
    "version": "1.0.0",
    "description": "Core functionality for Afriex SDK",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./client": {
            "import": "./dist/client/index.mjs",
            "require": "./dist/client/index.js",
            "types": "./dist/client/index.d.ts"
        },
        "./config": {
            "import": "./dist/config/index.mjs",
            "require": "./dist/config/index.js",
            "types": "./dist/config/index.d.ts"
        },
        "./errors": {
            "import": "./dist/errors/index.mjs",
            "require": "./dist/errors/index.js",
            "types": "./dist/errors/index.d.ts"
        },
        "./types": {
            "import": "./dist/types/index.mjs",
            "require": "./dist/types/index.js",
            "types": "./dist/types/index.d.ts"
        },
        "./utils": {
            "import": "./dist/utils/index.mjs",
            "require": "./dist/utils/index.js",
            "types": "./dist/utils/index.d.ts"
        }
    },
    "scripts": {
        "build": "tsup src/index.ts src/client/index.ts src/config/index.ts src/errors/index.ts src/types/index.ts src/utils/index.ts --format cjs,esm --dts --clean",
        "type-check": "tsc --noEmit",
        "clean": "rimraf dist",
        "test": "jest"
    },
    "dependencies": {
        "axios": "^1.6.5"
    },
    "devDependencies": {
        "@types/jest": "^29.5.11",
        "jest": "^29.7.0",
        "rimraf": "^5.0.5",
        "ts-jest": "^29.1.2",
        "tsup": "^8.0.1",
        "typescript": "^5.3.3"
    },
    "peerDependencies": {
        "typescript": ">=5.0.0"
    },
    "files": [
        "dist",
        "README.md"
    ],
    "publishConfig": {
        "access": "public"
    },
    "license": "MIT"
}